{
  "presets": [
    "@vue/cli-plugin-babel/preset",
    [
      "@babel/env",
      {
        "modules": false,
        "corejs": {
          "version": 3
        },
        "targets": {
          "ie": 9,
          "browsers": "> 0.5%"
        },
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": ["@babel/transform-runtime"]
}
